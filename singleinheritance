class animal{
	void eat(){
		System.out.println("Animal eats");
		}
	}
class dog extends animal{
	void bark(){
		System.out.println("dogs bark");
		}
	}
public class singleinheritance{
	public static void main(String[] args){
		dog d=new dog();
		d.eat();
		d.bark();
		}
	}
output:Animal eats
       dogs bark
